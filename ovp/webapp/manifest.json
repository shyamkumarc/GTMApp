{
	"_version": "1.7.0",
	"start_url": "start.html",
	"sap.app": {
		"id": "ovp.ovp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{app_title}}",
		"description": "{{app_description}}",
		"dataSources": {
			"catalog": {
				"uri": "/srv_api/v2/catalog/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/catalog/metadata.xml",
					"annotations": [
						"annotation0"
					]
				}
			},
			"annotation0": {
				"type": "ODataAnnotation",
				"uri": "annotation0.xml",
				"settings": {
					"localUri": "annotation0.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "OVP.cardtemplate",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.38.34",
			"libs": {
				"sap.ovp": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"@i18n": {
				"preload": true,
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"catalog": {
				"dataSource": "catalog",
				"settings": {
					"defaultCountMode": "Inline"
				}
			}
		},
		"extends": {
			"extensions": {}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.ovp": {
		"globalFilterModel": "catalog",
		"globalFilterEntityType": "Packages",
		"enableLiveFilter": true,
		"considerAnalyticalParameters": false,
		"cards": {
			"card9": {
				"model": "catalog",
				"template": "sap.ovp.cards.stack",
				"settings": {
					"title": "testcard",
					"entitySet": "Packages",
					"subTitle": "Open orders to approve",
					"objectStreamCardsSettings":{
						"annotationPath": "com.sap.vocabularies.UI.v1.Facets#stack"
					}
				}
			}
		}
	}
}